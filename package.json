{
  "name": "@zazen/semantic-release",
  "description": "Automate releases, free of weariness and confusion",
  "license": "ISC",
  "author": "Jeff <rustyangel@gmail.com> (https://tidaltheory.co)",
  "homepage": "https://github.com/stormwarning/zazen-semantic-release",
  "repository": "stormwarning/zazen-semantic-release",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "prettier-eslint '**/*.js' --list-different",
    "test": "npm run lint"
  },
  "devDependencies": {
    "@zazen/eslint-config": "^0.3.0",
    "eslint": "^5.8.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier-eslint-cli": "^4.7.1"
  },
  "keywords": [
    "semantic-release",
    "shareable config"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
