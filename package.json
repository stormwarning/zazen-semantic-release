{
  "name": "@zazen/semantic-release",
  "description": "Automate releases, free of weariness and confusion",
  "license": "ISC",
  "author": "Jeff <rustyangel@gmail.com> (https://tidaltheory.co)",
  "homepage": "https://github.com/stormwarning/zazen-semantic-release",
  "repository": "stormwarning/zazen-semantic-release",
  "version": "0.1.2",
  "main": "index.js",
  "scripts": {
    "lint": "prettier-eslint '**/*.js' --list-different",
    "test": "npm run lint"
  },
  "dependencies": {
    "@semantic-release/changelog": "^3.0.5",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.4.2",
    "@semantic-release/npm": "^5.1.13",
    "@zazen/conventional-changelog": "^1.0.5"
  },
  "peerDependencies": {
    "semantic-release": "^15.0.0"
  },
  "devDependencies": {
    "@zazen/eslint-config": "^0.3.0",
    "eslint": "^6.1.0",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "prettier-eslint-cli": "^4.7.1",
    "semantic-release": "^15.13.19",
    "travis-deploy-once": "^5.0.11"
  },
  "keywords": [
    "semantic-release",
    "shareable config"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "release": {
    "extends": "./index.js"
  }
}
